{
  "ignoreFiles": [
    ".gitignore"
  ],
  "ignoreDirs": [
    "node_modules",
    ".git"
  ],
  "rules": [
    {
      "type": "file",
      "name": ".directoryvalidator.json",
      "isOptional": true
    },
    {
      "type": "file",
      "name": "package.json"
    },
    {
      "type": "directory",
      "name": "src",
      "rules": []
    }
  ]
}
